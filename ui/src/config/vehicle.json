{
    "title": "Vehicle Management",
    "layout": {
      "header": {
        "enabled": true,
        "logo": "/images/company-logo.png",
        "style": {
          "background": "#fff",
          "padding": "0 24px",
          "boxShadow": "0 2px 8px rgba(0,0,0,0.1)",
          "height": "64px",
          "lineHeight": "64px"
        }
      },
      "sidebar": {
        "enabled": true,
        "collapsed": false,
        "width": 256,
        "collapsedWidth": 80,
        "theme": "dark",
        "style": {
          "background": "#001529",
          "boxShadow": "2px 0 8px 0 rgba(29,35,41,.05)"
        },
        "menu": {
          "items": [
            { "key": "dashboard", "label": "Dashboard", "icon": "DashboardOutlined", "path": "/dashboard" },
            {
              "type": "group",
              "label": "Management",
              "children": [
                { "key": "employees", "label": "Employees", "icon": "UserOutlined", "path": "/employees" },
                { "key": "vehicles", "label": "Vehicles", "icon": "CarOutlined", "path": "/vehicles" },
                { "key": "trips", "label": "Trips", "icon": "CompassOutlined", "path": "/trips" }
              ]
            },
            {
              "type": "group",
              "label": "Administration",
              "children": [
                { "key": "roles", "label": "Roles & Permissions", "icon": "SafetyOutlined", "path": "/roles" },
                { "key": "settings", "label": "Settings", "icon": "SettingOutlined", "path": "/settings" }
              ]
            },
            {
              "type": "group",
              "label": "Reports",
              "children": [
                { "key": "analytics", "label": "Analytics", "icon": "BarChartOutlined", "path": "/analytics" },
                { "key": "expenses", "label": "Expenses", "icon": "DollarOutlined", "path": "/expenses" },
                { "key": "maintenance", "label": "Maintenance", "icon": "ToolOutlined", "path": "/maintenance" }
              ]
            }
          ]
        },
        "footer": {
          "enabled": true,
          "style": {
            "padding": "16px",
            "background": "rgba(0, 0, 0, 0.25)",
            "color": "#fff",
            "height": "80px"
          },
          "content": {
            "expanded": { "avatar": "/images/user-avatar.png", "name": "John Doe", "role": "Admin" },
            "collapsed": { "avatar": "/images/user-avatar.png" }
          }
        }
      },
      "content": {
        "style": {
          "minHeight": "calc(100vh - 64px)",
          "background": "#f0f2f5"
        }
      },
      "footer": {
        "enabled": true,
        "text": "Â© 2024 My Application. All rights reserved.",
        "style": {
          "textAlign": "center",
          "padding": "16px",
          "background": "#f7f7f7"
        },
        "links": [
          { "text": "Privacy Policy", "url": "/privacy" },
          { "text": "Terms", "url": "/terms" }
        ]
      }
    },
    "sections": [
      {
        "type": "banner",
        "style": {
          "background": "#52c41a",
          "padding": "24px",
          "marginBottom": "24px"
        },
        "content": {
          "title": "Vehicle Management",
          "description": "Manage your organization's vehicle fleet",
          "style": {
            "maxWidth": "100%",
            "padding": "0 24px"
          }
        }
      },
      {
        "type": "form",
        "containerStyle": {
          "padding": "0 24px"
        },
        "wrapperStyle": {
          "background": "#ffffff",
          "padding": "24px",
          "borderRadius": "8px",
          "boxShadow": "0 2px 8px rgba(0,0,0,0.1)"
        },
        "title": "Vehicle List",
        "subtitle": "View and manage vehicles in your fleet",
        "layout": {
          "type": "vertical"
        },
        "table": {
          "enabled": true,
          "columns": [
            { 
              "title": "Vehicle ID", 
              "dataIndex": "vehicleId", 
              "key": "vehicleId",
              "width": "15%"
            },
            { 
              "title": "Make/Model", 
              "dataIndex": "makeModel", 
              "key": "makeModel",
              "width": "20%"
            },
            { 
              "title": "Year", 
              "dataIndex": "year", 
              "key": "year",
              "width": "10%"
            },
            { 
              "title": "Status", 
              "dataIndex": "status", 
              "key": "status",
              "width": "15%",
              "render": {
                "type": "tag",
                "colorMap": {
                  "Available": "green",
                  "In Use": "blue",
                  "Maintenance": "orange",
                  "Out of Service": "red"
                }
              }
            },
            { 
              "title": "Last Service", 
              "dataIndex": "lastService", 
              "key": "lastService",
              "width": "15%"
            },
            {
              "title": "Actions",
              "key": "actions",
              "width": "15%",
              "render": {
                "type": "actions",
                "items": [
                  {
                    "type": "button",
                    "buttonProps": {
                      "type": "link",
                      "icon": "EditOutlined"
                    },
                    "onClick": {
                      "type": "modal",
                      "modalId": "editVehicleModal"
                    }
                  },
                  {
                    "type": "button",
                    "buttonProps": {
                      "type": "link",
                      "danger": true,
                      "icon": "DeleteOutlined"
                    },
                    "onClick": {
                      "type": "modal",
                      "modalId": "deleteVehicleModal"
                    }
                  }
                ]
              }
            }
          ],
          "api": {
            "endpoint": "/api/vehicles",
            "method": "GET",
            "headers": {
              "Content-Type": "application/json"
            }
          },
          "fallbackData": [
            {
              "id": "1",
              "vehicleId": "VH001",
              "makeModel": "Toyota Camry",
              "year": "2022",
              "status": "Available",
              "lastService": "2024-01-15"
            },
            {
              "id": "2",
              "vehicleId": "VH002",
              "makeModel": "Honda CR-V",
              "year": "2023",
              "status": "In Use",
              "lastService": "2024-02-01"
            }
          ]
        },
        "actions": [
          {
            "label": "Add Vehicle",
            "buttonProps": {
              "type": "primary",
              "icon": "PlusOutlined"
            },
            "style": {
              "marginBottom": "16px"
            },
            "onClick": {
              "type": "modal",
              "modalId": "addVehicleModal"
            }
          }
        ]
      }
    ],
    "modals": {
      "addVehicleModal": {
        "title": "Add New Vehicle",
        "layout": {
          "type": "vertical"
        },
        "fields": [
          {
            "name": "vehicleId",
            "type": "text",
            "label": "Vehicle ID",
            "placeholder": "Enter vehicle ID",
            "size": "large",
            "rules": [{ "required": true, "message": "Please enter vehicle ID" }]
          },
          {
            "name": "makeModel",
            "type": "text",
            "label": "Make/Model",
            "placeholder": "Enter make and model",
            "size": "large",
            "rules": [{ "required": true, "message": "Please enter make and model" }]
          },
          {
            "name": "year",
            "type": "select",
            "label": "Year",
            "placeholder": "Select year",
            "size": "large",
            "options": [
              { "label": "2024", "value": "2024" },
              { "label": "2023", "value": "2023" },
              { "label": "2022", "value": "2022" },
              { "label": "2021", "value": "2021" }
            ],
            "rules": [{ "required": true, "message": "Please select year" }]
          },
          {
            "name": "status",
            "type": "select",
            "label": "Status",
            "placeholder": "Select status",
            "size": "large",
            "options": [
              { "label": "Available", "value": "Available" },
              { "label": "In Use", "value": "In Use" },
              { "label": "Maintenance", "value": "Maintenance" },
              { "label": "Out of Service", "value": "Out of Service" }
            ],
            "rules": [{ "required": true, "message": "Please select status" }]
          },
          {
            "name": "lastService",
            "type": "date",
            "label": "Last Service Date",
            "placeholder": "Select last service date",
            "size": "large",
            "rules": [{ "required": true, "message": "Please select last service date" }]
          }
        ],
        "actions": [
          {
            "label": "Add Vehicle",
            "buttonProps": {
              "type": "primary",
              "htmlType": "submit",
              "block": true
            },
            "style": {
              "marginTop": "24px"
            },
            "api": {
              "endpoint": "/api/vehicles",
              "method": "POST",
              "headers": {
                "Content-Type": "application/json"
              }
            },
            "messages": {
              "success": "Vehicle added successfully",
              "failure": "Failed to add vehicle"
            }
          }
        ]
      },
      "editVehicleModal": {
        "title": "Edit Vehicle",
        "layout": {
          "type": "vertical"
        },
        "fields": [
          {
            "name": "vehicleId",
            "type": "text",
            "label": "Vehicle ID",
            "placeholder": "Enter vehicle ID",
            "size": "large",
            "rules": [{ "required": true, "message": "Please enter vehicle ID" }]
          },
          {
            "name": "makeModel",
            "type": "text",
            "label": "Make/Model",
            "placeholder": "Enter make and model",
            "size": "large",
            "rules": [{ "required": true, "message": "Please enter make and model" }]
          },
          {
            "name": "year",
            "type": "select",
            "label": "Year",
            "placeholder": "Select year",
            "size": "large",
            "options": [
              { "label": "2024", "value": "2024" },
              { "label": "2023", "value": "2023" },
              { "label": "2022", "value": "2022" },
              { "label": "2021", "value": "2021" }
            ],
            "rules": [{ "required": true, "message": "Please select year" }]
          },
          {
            "name": "status",
            "type": "select",
            "label": "Status",
            "placeholder": "Select status",
            "size": "large",
            "options": [
              { "label": "Available", "value": "Available" },
              { "label": "In Use", "value": "In Use" },
              { "label": "Maintenance", "value": "Maintenance" },
              { "label": "Out of Service", "value": "Out of Service" }
            ],
            "rules": [{ "required": true, "message": "Please select status" }]
          },
          {
            "name": "lastService",
            "type": "date",
            "label": "Last Service Date",
            "placeholder": "Select last service date",
            "size": "large",
            "rules": [{ "required": true, "message": "Please select last service date" }]
          }
        ],
        "actions": [
          {
            "label": "Update Vehicle",
            "buttonProps": {
              "type": "primary",
              "htmlType": "submit",
              "block": true
            },
            "style": {
              "marginTop": "24px"
            },
            "api": {
              "endpoint": "/api/vehicles/{id}",
              "method": "PUT",
              "headers": {
                "Content-Type": "application/json"
              }
            },
            "messages": {
              "success": "Vehicle updated successfully",
              "failure": "Failed to update vehicle"
            }
          }
        ]
      },
      "deleteVehicleModal": {
        "title": "Delete Vehicle",
        "type": "confirm",
        "content": "Are you sure you want to delete this vehicle? This action cannot be undone.",
        "actions": [
          {
            "label": "Delete",
            "buttonProps": {
              "type": "primary",
              "danger": true
            },
            "api": {
              "endpoint": "/api/vehicles/{id}",
              "method": "DELETE",
              "headers": {
                "Content-Type": "application/json"
              }
            },
            "messages": {
              "success": "Vehicle deleted successfully",
              "failure": "Failed to delete vehicle"
            }
          }
        ]
      }
    }
  }