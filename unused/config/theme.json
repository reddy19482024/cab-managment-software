      "zIndex": {
        "drawer": 1000,
        "dropdown": 1010,
        "sticky": 1020,
        "fixed": 1030,
        "modal": 1040,
        "popover": 1050,
        "tooltip": 1060
      },
      "components": {
        "button": {
          "borderRadius": "6px",
