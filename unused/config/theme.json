{
    "theme": {
      "mode": "light",
      "colors": {
        "primary": "#1890ff",
        "secondary": "#52c41a",
        "success": "#52c41a",
        "warning": "#faad14",
        "error": "#ff4d4f",
        "info": "#1890ff",
        "background": {
          "default": "#f0f2f5",
          "paper": "#ffffff",
          "layout": "#f5f5f5"
        },
        "text": {
          "primary": "#1a1a1a",
          "secondary": "#666666",
          "disabled": "#bfbfbf"
        },
        "border": {
          "default": "#d9d9d9",
          "light": "#f0f0f0",
          "dark": "#8c8c8c"
        }
      },
      "typography": {
        "fontFamily": "Inter, system-ui, sans-serif",
        "scale": {
          "xs": "12px",
          "sm": "14px",
          "md": "16px",
          "lg": "18px",
          "xl": "20px",
          "xxl": "24px",
          "xxxl": "30px"
        },
        "weight": {
          "light": 300,
          "regular": 400,
          "medium": 500,
          "semibold": 600,
          "bold": 700
