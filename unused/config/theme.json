      "breakpoints": {
        "xs": "480px",
        "sm": "576px",
        "md": "768px",
        "lg": "992px",
        "xl": "1200px",
        "xxl": "1600px"
      },
      "transitions": {
        "duration": {
          "fast": "0.2s",
          "normal": "0.3s",
          "slow": "0.4s"
