        "lineHeight": {
          "tight": 1.25,
          "normal": 1.5,
          "relaxed": 1.75
        }
      },
      "spacing": {
        "unit": 8,
        "xs": "4px",
        "sm": "8px",
        "md": "16px",
        "lg": "24px",
        "xl": "32px",
        "xxl": "48px",
        "layout": {
          "headerHeight": "64px",
          "sidebarWidth": "280px",
          "footerHeight": "48px",
          "containerPadding": "24px"
