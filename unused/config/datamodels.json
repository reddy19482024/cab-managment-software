{
    "models": {
      "Employee": {
        "employeeId": {
          "type": "String",
          "required": true,
          "unique": true,
          "trim": true
        },
        "password": {
          "type": "String",
          "required": true
        },
        "role": {
          "type": "String",
          "enum": ["admin", "employee"],
          "required": true,
          "default": "employee"
        },
        "name": {
          "type": "String",
          "required": true,
          "trim": true
        },
        "email": {
          "type": "String",
          "required": true,
          "unique": true,
          "trim": true
        },
        "phone": {
          "type": "String",
          "required": true,
          "trim": true
        },
        "department": {
          "type": "String",
          "required": true,
          "trim": true
        },
        "pickupAddress": {
          "address": {
            "type": "String",
            "required": true
          },
          "landmark": {
            "type": "String"
          },
          "location": {
            "type": "Point",
            "coordinates": ["Number"]
          }
        },
        "routeId": {
          "type": "ObjectId",
          "ref": "Route"
        },
        "status": {
          "type": "String",
          "enum": ["active", "inactive"],
          "default": "active"
        },
        "lastLogin": {
          "type": "Date"
        },
        "createdAt": {
          "type": "Date",
          "default": "Date.now"
        },
        "updatedAt": {
          "type": "Date"
        }
      },
      "Route": {
        "routeName": {
          "type": "String",
          "required": true,
          "trim": true
        },
        "startPoint": {
          "address": {
            "type": "String",
            "required": true
          },
          "location": {
            "type": "Point",
            "coordinates": ["Number"]
          }
        },
        "endPoint": {
          "address": {
            "type": "String",
            "required": true
          },
          "location": {
            "type": "Point",
            "coordinates": ["Number"]
          }
        },
        "stops": [{
          "order": {
            "type": "Number",
            "required": true
          },
          "address": {
            "type": "String",
            "required": true
          },
          "location": {
            "type": "Point",
            "coordinates": ["Number"]
          }
        }],
        "vehicleId": {
          "type": "ObjectId",
          "ref": "Vehicle",
          "required": true
        },
        "status": {
          "type": "String",
          "enum": ["active", "inactive"],
          "default": "active"
        },
        "createdAt": {
          "type": "Date",
          "default": "Date.now"
        },
        "updatedAt": {
          "type": "Date"
        }
      },
      "Vehicle": {
        "vehicleNumber": {
          "type": "String",
          "required": true,
          "unique": true,
          "trim": true
        },
        "type": {
          "type": "String",
          "enum": ["bus", "van", "car"],
          "required": true
        },
        "capacity": {
          "type": "Number",
          "required": true,
          "min": 1
        },
        "status": {
          "type": "String",
          "enum": ["active", "maintenance", "inactive"],
          "default": "active"
        },
        "createdAt": {
          "type": "Date",
          "default": "Date.now"
        },
        "updatedAt": {
          "type": "Date"
        }
      },
      "Trip": {
        "routeId": {
          "type": "ObjectId",
          "ref": "Route",
          "required": true
        },
        "vehicleId": {
          "type": "ObjectId",
          "ref": "Vehicle",
          "required": true
        },
        "date": {
          "type": "Date",
          "required": true
        },
        "startTime": {
          "type": "Date",
          "required": true
        },
        "endTime": {
          "type": "Date"
        },
        "status": {
          "type": "String",
          "enum": ["scheduled", "in-progress", "completed", "cancelled"],
          "default": "scheduled"
        },
        "attendance": [{
          "employeeId": {
            "type": "ObjectId",
            "ref": "Employee",
            "required": true
          },
          "status": {
            "type": "String",
            "enum": ["present", "absent", "late"],
            "required": true
          },
          "pickupTime": {
            "type": "Date"
          }
        }],
        "createdAt": {
          "type": "Date",
          "default": "Date.now"
        },
        "updatedAt": {
          "type": "Date"
        }
      }
    }
  }